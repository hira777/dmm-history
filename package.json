    "@types/chrome": "0.0.91",
    "@types/lodash.orderby": "^4.6.6",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "babel-jest": "^24.8.0",
    "css-loader": "^2.1.1",
    "eslint": "^6.7.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.0.1",
    "jest": "^24.8.0",
    "node-sass": "^4.9.0",
    "pug": "^2.0.0-rc.3",
    "pug-loader": "^2.3.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "@types/jest": "^24.0.23",
    "bulma": "^0.7.0",
    "date-fns": "^1.29.0",
    "lodash.orderby": "^4.6.0",
    "vue": "^2.5.16",
    "vuex": "^3.0.1"
  }
