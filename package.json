  "scripts": {
    "check-types": "tsc -p ./chrome/tsconfig.json --noEmit",
    "dev": "cd chrome && webpack --mode development --watch --hide-modules",
    "build": "cd chrome && webpack --mode production --progress --hide-modules",
    "zip": "zip -r -FS ./dist/chrome.zip chrome -x *src/* -x *webpack.config.js",
    "release": "node release.js && yarn run build && yarn run zip",
    "lint": "eslint chrome/src --fix --ext=ts",
    "test": "jest"
  },
