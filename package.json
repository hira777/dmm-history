    "babel-preset-env": "^1.7.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-vue": "^4.4.0",
    "node-sass": "^4.9.0",
    "pug": "^2.0.0-rc.3",
    "pug-loader": "^2.3.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.1.3"
  },
  "dependencies": {
    "bulma": "^0.7.0",
